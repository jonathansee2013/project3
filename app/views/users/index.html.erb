<div class="container">
  <form>
    <div class="row">

      <div class="col-md-4"></div>

      <div class="col-md-4">

        <h1>All Users</h1>

        <ul>
          <% @users.each do |u| %>
            <li>
              <%= link_to u.username, user_path(u.id) %>
              <% if u.is_admin %>
              (admin)
              <% end %>
            </li>
          <% end %>
        </ul>

        <div class="col-md-4"></div>

    </div> <!-- row -->
  </form>
</div> <!--container -->
